 
package org.jmedikit.plugin.gui.handlers;

import javax.inject.Inject;

import org.eclipse.e4.core.di.annotations.Execute;
import org.eclipse.e4.ui.model.application.MApplication;
import org.eclipse.e4.ui.model.application.ui.menu.MToolControl;
import org.eclipse.e4.ui.workbench.modeling.EModelService;
import org.jmedikit.plugin.gui.ImageViewPart;

public class ToolDicomTagsHandler {
	
	@Inject
	EModelService service;
	
	@Inject
	MApplication app;
	
	@Execute
	public void execute() {
		MToolControl toolcontrol = (MToolControl) service.find("org.jmedikit.plugin.imageview", app);
		System.out.println(toolcontrol.getObject().getClass().getName());
	}
		
}