package org.jmedikit.plugin.gui.handlers;

import org.eclipse.e4.core.di.annotations.Execute;
import org.eclipse.swt.widgets.Composite;
import org.eclipse.swt.widgets.DirectoryDialog;
import org.eclipse.swt.widgets.Shell;

public class OpenLocationHandler {

	@Execute
	public void openLocatione(Composite parent){
		System.out.println("Open Location");
		DirectoryDialog dialog = new DirectoryDialog((Shell) parent);
	}
}
